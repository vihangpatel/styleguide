{
  "name": "markdown-to-jsx",
  "description": "Interprets markdown text and outputs a JSX equivalent.",
  "homepage": "http://probablyup.github.io/markdown-to-jsx",
  "license": "MIT",
  "version": "5.4.2",
  "engines": {
    "node": ">= 4"
  },
  "keywords": [
    "markdown",
    "react",
    "jsx",
    "remark"
  ],
  "author": "Evan Scott <probablyup@gmail.com>",
  "repository": "probablyup/markdown-to-jsx",
  "bugs": "https://github.com/probablyup/markdown-to-jsx/issues",
  "files": [
    "index.js",
    "index.cjs.js",
    "index.esm.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.cjs.js",
  "jsnext:main": "index.esm.js",
  "module": "index.esm.js",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "codecov": "^2.1.0",
    "in-publish": "^2.0.0",
    "jest": "^20.0.4",
    "polished": "^1.3.0",
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.1",
    "uglify-js": "^2.7.3",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "prop-types": "^15.5.10",
    "remark-parse": "^4.0.0",
    "unified": "^6.1.5"
  },
  "peerDependencies": {
    "react": ">= 0.14.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "scripts": {
    "prepublish": "in-publish && npm run build || not-in-publish",
    "build": "babel index.js --out-file index.cjs.js && BABEL_ENV=esm babel index.js --out-file index.esm.js",
    "release": "webpack --config ./webpack.release.config.babel.js -p --display-optimization-bailout",
    "start": "webpack-dev-server --hot --inline --open",
    "test": "jest --verbose"
  }
}
