		"lint:fix": "yarn lint --fix",
		"precommit": "lint-staged",
		"flow:defs": "flow-typed install --overwrite"
	},
	"lint-staged": {
		"*.{js,jsx}": ["lint:fix", "git add"],
		"*.{json,css}": ["prettier --tab-width 4 --use-tabs --write", "git add"]
	},
